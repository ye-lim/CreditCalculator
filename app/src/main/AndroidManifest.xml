<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.creditcalculator">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GetFileActivity"></activity>
        <activity android:name=".RecommandSubjectActivity" />
        <activity android:name=".SelfInfoModifyActivity" />
        <activity android:name=".SelfInfoActivity" />
        <activity android:name=".AdditionalInfoActivity" />
        <activity android:name=".CompletedSubListActivity" />
        <activity android:name=".CompleteSubActivity" />
        <activity android:name=".InfoInputActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>